<section class="artist-results">
    {{#each searchedArtist}}
        <div class="individual-artist">
            <h2 class="music-titles">{{name}}</h2> 
            <a href="/artist={{id}}/albums">
            <img src="{{images.0.url}}" onerror="this.onerror=null; this.src='/images/turntable.png' "height="200" width="200"/></a>
            <br>
            <a class="button-style" href="/artist={{id}}/albums">View Albums of {{name}}</a>
        </div>
    {{/each}}
    
</section>
<a href="javascript:history.back()" class="navigation-button">Search Another Artist</a>